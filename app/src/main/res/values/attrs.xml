<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiScrollView">
        <attr name="fillViewport" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CollapsingImageBehavior">
        <attr name="collapsedTarget" format="reference"/>
    </declare-styleable>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetBackgroundColor" format="color"/>
        <attr name="appWidgetTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="NoScrollableRecyclerView">
        <attr name="layoutOrientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="recyclerOrientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="reverseLayout" format="boolean"/>
    </declare-styleable>
</resources>